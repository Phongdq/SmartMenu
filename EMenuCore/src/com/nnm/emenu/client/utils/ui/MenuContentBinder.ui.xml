<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:f="urn:import:com.googlecode.mgwt.ui.client.widget.panel.flex"
	xmlns:s="urn:import:com.googlecode.mgwt.ui.client.widget.panel.scroll"
	xmlns:b="urn:import:com.googlecode.mgwt.ui.client.widget.button"
	xmlns:p="urn:import:com.googlecode.mgwt.ui.client.widget.panel">
	<ui:style>
	.btnHeader {
		size: portrait;
		width: 5%;
		height: 100%;
		margin-left: 5px;
		background-image: url("image/iconMenu.png");
		background-repeat: no-repeat;
		background-position: center;
		vertical-align: middle;
		text-align: center;
		font-size: 130%;
		font-weight: bold;
		color: black;
		cursor: pointer;
	}
	
	.lbHeader {
		color: #116388;
		font-size: 140%;
		height: auto;
		margin: auto;
		font-weight: bold;text-transform: none;
	}
	
	.content {
		border: none;
		background-color: transparent;
	}
	</ui:style>
	<f:FlexPanel width="100%" height="100%">
		<f:FlexPanel ui:field="header" width="100%" height="7%">
		</f:FlexPanel>
		<f:FlexPanel ui:field="menuContent" width="100%" height="93%">
			<s:ScrollPanel ui:field="scrollContent" width="100%"
				height="100%" scrollingEnabledX="false">
				<p:Panel ui:field="contentMenu" width="98%"></p:Panel>
			</s:ScrollPanel>
		</f:FlexPanel>
	</f:FlexPanel>
</ui:UiBinder> 